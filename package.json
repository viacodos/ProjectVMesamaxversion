{
    "name": "Project V redoing",
    "version": "1.0.0",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "overrides": {
        "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
    },
    "scripts": {
        "dev:user": "npm run dev --workspace=apps/frontend-user",
        "dev:admin": "npm run dev --workspace=apps/frontend-admin",
        "dev:backend": "npm run dev --workspace=apps/backend",
        "dev": "concurrently \"npm run dev:user\" \"npm run dev:admin\" \"npm run dev:backend\"",
        "docker:up": "docker-compose up -d",
        "docker:down": "docker-compose down",
        "docker:logs": "docker-compose logs -f mysql"
    },
    "devDependencies": {
        "autoprefixer": "^10.4.0",
        "concurrently": "^9.2.1",
        "lucide-react": "^0.562.0",
        "postcss": "^8.4.0",
        "react": "^19.2.3",
        "react-dom": "^19.2.3",
        "tailwindcss": "^3.4.0"
    },
    "dependencies": {
        "@radix-ui/react-dialog": "^1.1.15",
        "@radix-ui/react-dropdown-menu": "^2.1.16",
        "@radix-ui/react-label": "^2.1.8",
        "@radix-ui/react-slot": "^1.2.4",
        "@tabler/icons-react": "^3.36.1",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "framer-motion": "^12.26.2",
        "tailwind-merge": "^3.4.0",
        "tailwindcss-animate": "^1.0.7"
    }
}